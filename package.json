  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmuellr/kbn-action.git"
  },
  "dependencies": {
    "got": "^9.6.0",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "standard": "^12.0.1"
  },
  "standard": {
