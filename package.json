{
  "name": "kbn-action",
  "version": "1.0.0",
  "description": "command-line utility for working with Kibana actions",
  "main": "kbn-action.js",
  "author": "pmuellr",
  "bin": {
    "kbn-action": "kbn-action.js"
  },
